{
  "kind": "collectionType",
  "collectionName": "apartment_posts",
  "info": {
    "name": "apartmentPost",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "house": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "string",
      "required": true
    },
    "descriptions": {
      "type": "richtext"
    },
    "apartmentGallery": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "apartments_owner": {
      "model": "apartments-owner"
    },
    "x_services": {
      "collection": "x-services"
    },
    "chooseAction": {
      "type": "enumeration",
      "enum": [
        "Sell",
        "Rent"
      ],
      "required": true
    }
  }
}
