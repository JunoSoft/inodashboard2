{
  "kind": "collectionType",
  "collectionName": "car_posts",
  "info": {
    "name": "carPost",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "carName": {
      "type": "string",
      "required": true
    },
    "carType": {
      "type": "string",
      "required": true
    },
    "engine": {
      "type": "string",
      "required": true
    },
    "gas": {
      "type": "string",
      "required": true
    },
    "interialColor": {
      "type": "string",
      "required": true
    },
    "exterialColor": {
      "type": "string",
      "required": true
    },
    "milesAge": {
      "type": "string",
      "required": true
    },
    "postProfile": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "description": {
      "type": "richtext"
    },
    "car_owner": {
      "model": "car-owners"
    },
